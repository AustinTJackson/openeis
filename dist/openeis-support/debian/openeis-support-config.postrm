#!/bin/sh
# postrm script for openeis-support-config

set -e

case "$1" in
  purge)
    rm -f /etc/openeis/secret.key || true
  ;;

  remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
  ;;

  *)
    echo "postrm called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

#DEBHELPER#

exit 0
